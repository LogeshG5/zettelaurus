{
  "name": "@logeshg5/docusaurus-plugin-docs-editor",
  "version": "0.0.1",
  "description": "A WYSIWYG editor for docusaurus",
  "main": "src/index.js",
  "repository": "https://github.com/jlvandenhout/docusaurus-plugin-docs-editor",
  "author": "Logesh G <logeshg5@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.18",
    "@docusaurus/preset-classic": "^2.0.0-beta.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rich-markdown-editor": "^11.21.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
